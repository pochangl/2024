# 快取
cache = {} 

def fib2(n):
    # 前兩項
    if n == 0:
        return 0
    elif n == 1:
        return 1

    # 嘗試存取快取
    try:
        return cache[n]
    except KeyError:
        pass

    # 如果沒有就計算費式數列
    value = fib2(n -1) + fib2(n-2)

    # 把結果存到快取
    cache[n] = value

    # 回傳結果
    return value


fib2(1)
cache


fib2(2)
cache


fib2(3)
cache


fib2(100)


cache





